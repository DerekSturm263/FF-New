enum Direction
{
	Neutral,
	Up,
	Down,
	Left,
	Right
}

[Serializable]
struct DirectionalEmote
{
	EmoteMessageBinding Up;
	EmoteMessageBinding Down;
	EmoteMessageBinding Left;
	EmoteMessageBinding Right;
}

[Serializable]
struct DirectionalFPVector2
{
	FPVector2 Up;
	FPVector2 Down;
	FPVector2 Left;
	FPVector2 Right;
}

[Serializable]
struct DirectionalMoveRef
{
	MoveRef Neutral;
	MoveRef Horizontal;
	MoveRef Up;
	MoveRef Down;
}
